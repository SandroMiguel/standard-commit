{
  "name": "package.json-boilerplate",
  "version": "0.0.6",
  "description": "The inspirational package.json boilerplate",
  "license": "MIT",
  "author": {
    "name": "Sandro Miguel Marques",
    "email": "sandromiguel@sandromiguel.com",
    "url": "http://sandromiguel.com"
  },
  "keywords": [
    "git",
    "conventional",
    "commit",
    "husky",
    "commitlint",
    "commitizen",
    "standard-version"
  ],
  "repository": "git@github.com:SandroMiguel/standard-commit.git",
  "scripts": {
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.9",
    "standard-version": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
